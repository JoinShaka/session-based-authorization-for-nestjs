{
  "dependencies": {
    "@nestjs/common": "^6.6.3",
    "@nestjs/core": "^6.6.3",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.6.3",
    "@nestjs/typeorm": "6.1.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "dotenv": "^8.0.0",
    "express-session": "^1.16.2",
    "passport": "^0.4.0",
    "passport-google-oauth": "1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.18",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "description": "",
  "devDependencies": {
    "@nestjs/testing": "^6.6.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.4.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@trejgun/nest-authorization",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "entity:create": "npm run typeorm -- entity:create -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "prebuild": "rm -rf dist/",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:debug": "NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/index.js\"",
    "start:dev": "NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/index.js\"",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test jest --config jest.config.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "NODE_ENV=development ts-node ./node_modules/.bin/typeorm --config src/ormconfig.ts"
  },
  "version": "0.0.1"
}
